FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    "(bottom|inlet|outlet)" 
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    top
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
    
    floatingObject
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }

    frontAndBack
    {
        type empty;
    }
    
}

// ************************************************************************* //
